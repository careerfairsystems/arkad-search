<html>

<head>
    <title>Koordinator webUI</title>

    <link href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.7/yeti/bootstrap.min.css" rel="stylesheet">
    <meta charset="utf-8">
    <meta name=viewport content="width= device-width">

</head>

<body>
<div class="row">
       <div class="col-md-6 col-md-offset-3">
            <div class="page-header">
                <h1 style = "margin-top:150px;">Database content for arkad search</h1>
            </div>




              <table class="table table-striped table-hover ">
                <thead>
                    <tr>
                 <th>Name:</th>
                <th>Time:</th>
                <th>Info:</th>
                    </tr>
               </thead>
              <% for(var i=0; i < dbcontent.length; i++) { %>
              <tr>
               <td><%= dbcontent[i].name %></td>
               <td><%= dbcontent[i].time %></td>
               <td> <textarea readonly class="form-control" rows="1" id="textArea" style = "resize:none"><%= dbcontent[i].info %></textarea></td>
               <td> <a class="btn btn-primary">edit</a> <a class="btn btn-danger">delete</a> </td>
               </tr>
              <% } %>
              </table>


                    <form action='/logout' method='post'>
                        <button type="submit" class="btn btn-danger"> Logout </button>
                    </form>

      </div>
    </div>
    </div>
    </div>



    <script src="generatetable.js"></script>
</body>

</html>
